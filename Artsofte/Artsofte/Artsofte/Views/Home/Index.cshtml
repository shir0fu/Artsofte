@{
    ViewData["Title"] = "List Employees";
}
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHalpers
@model IEnumerable<Artsofte.DTO.EmployeeDTO>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<div class="text-center">
    <center><h1>A list of employees</h1></center>

    <a>ADD</a>
    <center>
        <table border="1">
            <tr>
                <th>Name</th>
                <th>Surname</th>
                <th>Age</th>
                <th>Departament</th>
                <th>Language</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr><td>@item.Name</td><td>@item.Surname</td><td>@item.Age</td><td>@item.Departament</td><td>@item.PogrammingLanguage</td></tr>
            }
        </table>
    </center>
</div>

